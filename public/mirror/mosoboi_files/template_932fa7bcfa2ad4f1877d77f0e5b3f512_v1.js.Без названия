; /* Start:"a:4:{s:4:"full";s:91:"/local/templates/.default/components/bitrix/menu/catalog_mobile_new/script.js?1676539293622";s:6:"source";s:77:"/local/templates/.default/components/bitrix/menu/catalog_mobile_new/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
