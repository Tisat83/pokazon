; /* Start:"a:4:{s:4:"full";s:71:"/bitrix/templates/mosoboi_ad/js/jquery.sticky-kit.min.js?16765393442798";s:6:"source";s:56:"/bitrix/templates/mosoboi_ad/js/jquery.sticky-kit.min.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
/*
 Sticky-kit v1.1.2 | WTFPL | Leaf Corcoran 2015 | http://leafo.net
