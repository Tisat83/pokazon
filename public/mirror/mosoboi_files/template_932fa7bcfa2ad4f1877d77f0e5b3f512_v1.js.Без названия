let compilation = {
	rights: false,
	current: 0,
	editing: 0,
	editingNew: 0,//,     
	items: [],
	init: function () {// ,  
		compilation.getRight();
		$('.popup.new_compilation .new_compilation_wrap .close').on('click', compilation.hidePopupNew);
		$('.popup.compilation_saved .compilation_saved_wrap .close').on('click', compilation.hidePopupSaved);
		$('.popup.compilation_saved .compilation_saved_wrap .content .copy span').on('click', compilation.copyPopupSavedLink);
	},
	initAfterRight: function () {
		console.log('initAfterRight');

		compilation.initCookie();
		if(compilation.editing)
			compilation.addCatalogItemsCheckboxes();
		compilation.addHeaderIcon();
		$('.compilation_head_fixed .container .buttons button.add').on('click', compilation.showPopupNew);
		$('.compilation_head_fixed .container .buttons button.save').on('click', compilation.saveCompilation);
		$(document).on('change','.product-item-container .product-item-wrap .compilation_add input',compilation.toggleCatalogItemCheckbox);
		$(document).on('change','.product-item-container .compilation_add input',compilation.toggleCatalogItemCheckbox);
		$(document).on('change','.collect_div .compilation_add input',compilation.toggleCatalogItemCheckbox);
		$('.popup.new_compilation .new_compilation_wrap .content .submit button').on('click', compilation.create);
	},
	getRight: function () {
		let request = {action: 'checkRights'};
		$.post('/compilations/include_areas/ajax.php', request, function (data) {
			if(data === true) {
				compilation.rights = true;
				compilation.initAfterRight();
			}
		}, 'json');
	},
	initCookie: function () {
		let id = compilation.getCookie('compilationCurrent');
		if(/^\/compilations\//.test(location.pathname)) {
			compilation.setNonEdit();
			compilation.setCookie('compilationCurrentItems', "[]");
		}
		if(/^\/compilations\/\d+\/edit\//.test(location.pathname))
			compilation.setEdit(1);
		compilation.editing = parseInt(compilation.getCookie('compilationEditing'));
		compilation.editingNew = parseInt(compilation.getCookie('compilationEditingNew'));

		console.log('compilation.editing', compilation.editing);
		console.log('id', id);

		if(compilation.editing && id) {
			compilation.items = JSON.parse(compilation.getCookie('compilationCurrentItems') || "[]");
			compilation.hideHeaderAddButton();
			let request = {action: 'get', id: id};
			$.post('/compilations/include_areas/ajax.php', request, function (data) {
				if (data.link.length) {
					let fullLink = 'https://www.mosoboi.ru'+data.link;
					compilation.current = data.id;
					
//					if(data.items !== undefined && !compilation.items.length) {
					if(data.items !== undefined && (compilation.items===null || !compilation.items.length)) {
						compilation.items = data.items;
						compilation.setCookie('compilationCurrentItems', JSON.stringify(compilation.items));
					}
					compilation.hidePopupNew();
					$('.compilation_head_fixed .container .link').text(fullLink).attr('href', data.link);
					$('.compilation_head_fixed .container .name span').html(data.name);
					var items_length = 0;
					if(compilation.items!==null){
						items_length = compilation.items.length;
					}
					$('.compilation_head_fixed .container .name i').text(items_length);
				}
			}, 'json');
		}else{
			compilation.setCookie('compilationCurrentItems', JSON.stringify([]));
			compilation.hideHeaderSaveButton();
		}
	},
	setEdit: function (isNew) {
		isNew = parseInt(isNew);
		compilation.setCookie('compilationEditing', 1);
		compilation.setCookie('compilationEditingNew', isNew);
	},
	setNonEdit: function () {
		compilation.setCookie('compilationEditing', 0);
	},
	addHeaderIcon: function () {
		$('.header-button__viewed').before('<a class="top-compilation" href="/compilations/"></a>');
		//$('.top .top-viewed').before('<a class="top-compilation" href="/compilations/"></a>');
		// $('.top .top-fav').after('<a class="top-compilation" href="/compilations/"></a>');
		//$('.top #city-block-click').width(80);
	},
	showPopupNew: function () {
		$('.popup.new_compilation').addClass('active');
	},
	hidePopupNew: function () {
		$('.popup.new_compilation').removeClass('active');
	},
	setContentPopupSaved: function (link) {
		let fullLink = 'https://www.mosoboi.ru'+link;
		$('.popup.compilation_saved .compilation_saved_wrap .content .copy a').attr('href', link).text(fullLink);
		$('.popup.compilation_saved .compilation_saved_wrap .content .copy textarea').val(fullLink);
		$('.popup.compilation_saved .compilation_saved_wrap .content .button a').attr('href', fullLink);
	},

	showPopupSaved: function () {
		$('.popup.compilation_saved').addClass('active');
	},
	hidePopupSaved: function () {
		$('.popup.compilation_saved').removeClass('active');
	},
	showHeaderAddButton: function () {
		$('.compilation_head_fixed .container .buttons button.add').show(0);
	},
	hideHeaderAddButton: function () {
		$('.compilation_head_fixed .container .buttons button.add').hide(0);
	},
	showHeaderSaveButton: function () {
		$('.compilation_head_fixed .container .buttons button.save').show(0);
	},
	hideHeaderSaveButton: function () {
		$('.compilation_head_fixed .container .buttons button.save').hide(0);
	},

	create: function () {
		let name = $('.popup.new_compilation .new_compilation_wrap .content .field input').val();
		if(!!name.length)
		{
			let description = $('.popup.new_compilation .new_compilation_wrap .content .field textarea').val();
			$('.compilation_head_fixed .container .name span').text(name);
			let request = {action: 'add', name:name, description:description, items: compilation.items};
			$.post('/compilations/include_areas/ajax.php', request, function (data) {
				if(data.link.length) {
					let fullLink = 'https://www.mosoboi.ru'+data.link;
					$('.compilation_head_fixed .container .link').text(fullLink).attr('href', data.link);
					compilation.current = data.id;
					compilation.setEdit(1);
					compilation.addCatalogItemsCheckboxes();
					compilation.hidePopupNew();
					compilation.saveCurrent();
				}
			}, 'json');
		}
		else
			$('.popup.new_compilation .new_compilation_wrap .content .field input').addClass('.error');

		compilation.hideHeaderAddButton();
		compilation.showHeaderSaveButton();
	},
	addCatalogItemsCheckboxes: function () {
		$('.product-item-container .product-item-wrap .product-item,.js_compilation_checkbox, [data-role="catalog-item"]').after(
			'<div class="compilation_add">\n'+
				'<input type="checkbox">\n'+
			'</div>'
		).each(function (i, el) {
			//console.log($(el));
			let id = parseInt($(el).prev('.ecommerce-info').attr('data-id'));
			if (!id)
				id = parseInt($(el).find('.ecommerce-info').attr('data-id'));

			$(el).next('.compilation_add').find('input').attr('data-id', id)
		});
		var items_length = 0;
		if(compilation.items!==null){
			items_length = compilation.items.length;
		}

		console.log(compilation.items);

		if(items_length>0){
			compilation.items.forEach(function(item, i, arr) {
				$('.product-item-container .product-item-wrap .compilation_add input[data-id='+item+']').attr('checked', 'checked');
				$('.collect_div .compilation_add input[data-id='+item+']').attr('checked', 'checked');
				$('.product-item-container .compilation_add input[data-id='+item+']').attr('checked', 'checked');
			});			
		}
	},
	toggleCatalogItemCheckbox: function (e) {
		if($(e.target).is(':checked'))
			compilation.addCatalogItem(e);
		else
			compilation.deleteCatalogItem(e);
		compilation.setCookie('compilationCurrentItems', JSON.stringify(compilation.items));
		$('.compilation_head_fixed .container .name i').text(compilation.items.length);
	},
	addCatalogItem: function (e) {
		let id = parseInt($(e.target).attr('data-id'));
		compilation.items.push(id);
	},
	deleteCatalogItem: function (e) {
		let id = parseInt($(e.target).attr('data-id'));
		let deleteKey = null;
		compilation.items.forEach(function(item, i, arr) {
			if(item === id) {
				deleteKey = i;
			}
		});
		if(deleteKey !== null)
			compilation.items.splice(deleteKey, 1);
	},
	saveCurrent: function () {
		if(compilation.current)
			compilation.setCookie('compilationCurrent', compilation.current);
	},
	saveCompilation: function () {
		if(compilation.current) {
			let request = {action: 'save', id: compilation.current, items: compilation.items};
			$.post('/compilations/include_areas/ajax.php', request, function (data) {
				if (data.link.length) {
					compilation.setContentPopupSaved(data.link);
					if(compilation.editingNew)
						compilation.showPopupSaved();
					else {
						window.location.assign(data.link+'edit/');
					}
				}
			}, 'json');
		}
		else
			compilation.showPopupNew();
	},
	copyPopupSavedLink: function () {
		$('.popup.compilation_saved .content .copy textarea').select();
		document.execCommand("copy");
		$('.popup.compilation_saved .content .copy a').addClass('copied');
		setTimeout(function () {
			$('.popup.compilation_saved .content .copy a').removeClass('copied');
		}, 3000);
	},

	getCookie: function(name) {
		let matches = document.cookie.match(new RegExp(
			"(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
		));
		return matches ? decodeURIComponent(matches[1]) : undefined;
	},
	setCookie: function(name, value) {
		let options = {
			path: '/',
		};

		if (options.expires instanceof Date) {
			options.expires = options.expires.toUTCString();
		}

		let updatedCookie = encodeURIComponent(name) + "=" + encodeURIComponent(value);

		for (let optionKey in options) {
			updatedCookie += "; " + optionKey;
			let optionValue = options[optionKey];
			if (optionValue !== true) {
				updatedCookie += "=" + optionValue;
			}
		}

		document.cookie = updatedCookie;
	},
};

$(document).ready(compilation.init);
/* End */
;
; /* Start:"a:4:{s:4:"full";s:84:"/bitrix/templates/mosoboi_ad/plugins/colorbox/jquery.colorbox.min.js?167653934411900";s:6:"source";s:68:"/bitrix/templates/mosoboi_ad/plugins/colorbox/jquery.colorbox.min.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
