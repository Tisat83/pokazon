$(document).ready(function () {
	$(document).on('click', ".fast_saw", function () {
		console.log('ajax');
		var nextId = 0, prevId = 0,
			prev_li = $(this).closest('li').prev('li'),
			next_li = $(this).closest('li').next('li');
		if(prev_li.length)
			prevId = prev_li.find('.fast_saw').attr('data-fast-id');
		if(next_li.length)
			nextId = next_li.find('.fast_saw').attr('data-fast-id');

		var prev_div = $(this).closest(".product-item-small-card").prev(".product-item-small-card"),
			next_div = $(this).closest(".product-item-small-card").next(".product-item-small-card");

		if(prev_div.length)
			prevId = prev_div.find('.fast_saw').attr('data-fast-id');
		if(next_div.length)
            nextId = next_div.find('.fast_saw').attr('data-fast-id');

		var iblock_id = '';
		if (typeof $(this).attr('data-iblock-id') != 'undefined') {
			iblock_id = $(this).data('iblock-id');
		}

		let link = '/ajax/form/see-fast' + iblock_id + '.php';
		if ($('body').hasClass('design_v2')) {
			link = '/ajax/form/see-fast-redesign' + iblock_id + '.php';
		}

		console.log(link);

		$.colorbox({
			href: link,
			data: {
				'iblockId': $(this).attr('data-iblock-id'),
				'productId': $(this).attr('data-fast-id'),
				'sectionId': $(this).attr('data-section-id'),
				'prevId': prevId,
				'nextId': nextId
			},
			className: 'fast',
			initialWidth: 0,
			initialHeight: 0,
			onOpen: function () {
				$("#cboxClose").css({opacity: 0});
			},
			onComplete: function () {
				$("#cboxClose").stop().animate({opacity: 1});
				yaGoalsFunction.event8();
			}
		});
	});
	$(document).on('click', "[data-product-click]", function (event) {

		if (!$(event.target).data("cancel-click") && !$(event.target).parents("[data-cancel-click]").length)
			window.location.assign($(this).data("product-click"));
	});
	$(document).on('click', ".prev-fast, .next-fast", function () {
		var nextId = 0, prevId = 0,
			prev_li = $('.fast_saw[data-fast-id='+$(this).attr('data-fast-id')+']').closest('li').prev('li'),
			next_li = $('.fast_saw[data-fast-id='+$(this).attr('data-fast-id')+']').closest('li').next('li');
		if(prev_li.length)
			prevId = prev_li.find('.fast_saw').attr('data-fast-id');
		if(next_li.length)
			nextId = next_li.find('.fast_saw').attr('data-fast-id');

        var prev_div = $('.fast_saw[data-fast-id='+$(this).attr('data-fast-id')+']').closest(".product-item-small-card").prev(".product-item-small-card"),
            next_div = $('.fast_saw[data-fast-id='+$(this).attr('data-fast-id')+']').closest(".product-item-small-card").next(".product-item-small-card");

        if(prev_div.length)
            prevId = prev_div.find('.fast_saw').attr('data-fast-id');
        if(next_div.length)
            nextId = next_div.find('.fast_saw').attr('data-fast-id');

		var iblock_id = '';
		if (typeof $(this).attr('data-iblock-id') != 'undefined') {
			iblock_id = $(this).data('iblock-id');
		}

		let link = '/ajax/form/see-fast' + iblock_id + '.php';
		if ($('body').hasClass('design_v2')) {
			link = '/ajax/form/see-fast-redesign' + iblock_id + '.php';
		}

		$.colorbox({
			href: link,
			data: {
				'iblockId': $(this).attr('data-iblock-id'),
				'productId': $(this).attr('data-fast-id'),
				'sectionId': $(this).attr('data-section-id'),
				'prevId': prevId,
				'nextId': nextId
			},
			className: 'fast',
			initialWidth: 0,
			initialHeight: 0,
			onOpen: function () {
				$("#cboxClose").css({opacity: 0});
			},
			onComplete: function () {
				$("#cboxClose").stop().animate({opacity: 1});
			}
		});
	});
});
