yaCommerce = {
	dataLayer: window.dataLayer,
	errors: [],
	products: [],
	addProduct: function (product) {
		let addedProduct = {
			id: product.id,
			name: product.name,
		};
		if(typeof product.brand === "string")
			addedProduct.brand = product.brand;
		if(typeof product.category === "string")
			addedProduct.category = product.category;
		if(typeof product.coupon === "string")
			addedProduct.coupon = product.coupon;
		if(typeof product.discount === "number")
			addedProduct.discount = product.discount;
		if(typeof product.list === "string")
			addedProduct.list = product.list;
		if(typeof product.position === "number")
			addedProduct.position = product.position;
		if(typeof product.price === "number")
			addedProduct.price = product.price;
		if(typeof product.quantity === "number")
			addedProduct.quantity = product.quantity;
		if(typeof product.variant === "string")
			addedProduct.variant = product.variant;
		yaCommerce.products.push(addedProduct);
	},
	checkProducts: function (products) {
		let result = true;
		if(typeof products === 'object')
		{
			for (i in products)
				if(!yaCommerce.checkProduct(products[i]))
					result = false;
		}
		else
			yaCommerce.errors.push('Product list is not object');
		if(yaCommerce.errors.length) {
			yaCommerce.showErrors(products);
			return false;
		}
		return true;
	},
	checkProduct: function (product) {
		let errors = [];
		if(typeof product === 'object')
		{
			if((typeof product.id !== 'number' && typeof product.id !== 'string') || product.id.length < 1)
				errors.push('Product.id is required');
			if(typeof product.name !== 'string' || product.name.length < 1)
				errors.push('Product.name is required');
		}
		else
			errors.push('Product is not object');
		if(errors.length > 0) {
			yaCommerce.errors.push(...errors);
			return false;
		}
		return true;
	},
	showErrors: function (products) {
		if(yaCommerce.errors.length)
			console.log('ECommerce error!', yaCommerce.errors, products);
		yaCommerce.errors = [];
	},
	impressions: function (products = null) {
		if(typeof products !== "object" || products.length < 1)
			products = yaCommerce.products;
		if(yaCommerce.checkProducts(products))
			yaCommerce.dataLayer.push({
				"ecommerce": {
					"currencyCode": "RUB",
					"impressions": products
				}
			});
		yaCommerce.products = [];
	},
	click: function (product) {
		let products = [];
		if(typeof product !== "object")
			products = yaCommerce.products;
		else
			products.push(product);
		if(yaCommerce.checkProducts(products))
			yaCommerce.dataLayer.push({
				"ecommerce": {
					"currencyCode": "RUB",
					"click": {
						products:products
					}
				}
			});
		yaCommerce.products = [];
	},
	detail: function (product) {
		let products = [];
		if(typeof product !== "object")
			products = yaCommerce.products;
		else
			products.push(product);
		if(yaCommerce.checkProducts(products))
			yaCommerce.dataLayer.push({
				"ecommerce": {
					"currencyCode": "RUB",
					"detail": {
						products:products
					}
				}
			});
		yaCommerce.products = [];
	},
	add: function (product) {
		let products = [];
		if(typeof product !== "object")
			products = yaCommerce.products;
		else
			products.push(product);
		if(yaCommerce.checkProducts(products))
			yaCommerce.dataLayer.push({
				"ecommerce": {
					"currencyCode": "RUB",
					"add": {
						products:products
					}
				}
			});
		yaCommerce.products = [];
	},
	remove: function (product) {
		let products = [];
		if(typeof product !== "object")
			products = yaCommerce.products;
		else
			products.push(product);
		if(yaCommerce.checkProducts(products))
			yaCommerce.dataLayer.push({
				"ecommerce": {
					"currencyCode": "RUB",
					"remove": {
						products:products
					}
				}
			});
		yaCommerce.products = [];
	},
	purchase: function (orderId, products) {
		if(typeof products !== "object" || products.length < 1)
			products = yaCommerce.products;
		if(yaCommerce.checkProducts(products))
			yaCommerce.dataLayer.push({
				"ecommerce": {
					"currencyCode": "RUB",
					"purchase": {
						actionField: {id: orderId},
						products:products
					}
				}
			});
		yaCommerce.products = [];
	},
};

yaGoals = {
	send: function (target) {
		if(typeof ym === "function") {
			ym(15883444, 'reachGoal', target);
		}
	}
}


$(document).ready(function () {
	if(window.location.pathname == '/login/') {
		$('.login-form .fields form .field.field-button input[type="submit"]').on('click', function () {
			yaGoalsFunction.event1();
		})
		$('.register-form .fields form .field.field-button input[type="submit"]').on('click', function () {
			yaGoalsFunction.event2();
		})
	}
	$('#header .top-callback,.header .top-callback').on('click', function () {
		yaGoalsFunction.event3();
	});

	$('#header #tcShowCity .select-city').on('click', function () {
		yaGoalsFunction.event4();
	});

	$(document).on('click', '.spec_it', function () {
		yaGoalsFunction.event5();
	});

	$(document).on('click', '.add2basket', function () {
		yaGoalsFunction.event16();
	});

	$(document).on('click', '.companions .block', function () {
		yaGoalsFunction.event22();
	});

	$(document).on('click', '.companion-big .add2basket', function () {
		yaGoalsFunction.event23();
	});

	$(document).on('click', '.offer .block', function () {
		yaGoalsFunction.event24();
	});

	$(document).on('click', '.third-line .add2basket', function () {
		yaGoalsFunction.event25();
	});
	$(document).on('click', '#send_me_photo', function () {
		yaGoalsFunction.event28();
	});
	$(document).on('submit', '#contacts_form form', function () {
		yaGoalsFunction.event29();
	});
	$(document).on('click', '#faq_block .faq_item_link', function () {
		yaGoals.send('clckchitatdalee');
	});
	$(document).on('click', '.ya_clckdrygieoboi', function () {
		yaGoalsFunction.event30();
	});


	$(document).on('click', '.banner_area .js_yagoal_clcksotr', function () {
		yaGoals.send('clcksotr');
	});
